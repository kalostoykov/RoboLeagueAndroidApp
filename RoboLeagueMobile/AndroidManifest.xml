<!--       	    </intent-filter> -->
<!--       	</activity> -->
      	
      	<activity android:name="bg.roboleague.mobile.StartingPoint"
      	    	  android:label="@string/app_name"
                android:configChanges="orientation">
      	    <intent-filter>
      	        <action android:name="android.intent.action.MAIN"/>
      	        <category android:name="android.intent.category.LAUNCHER"/>
      	    </intent-filter>
      	</activity>
      	
      	<activity android:name="bg.roboleague.mobile.DeviceScanner"
      	    	  android:label="@string/app_name"
                android:configChanges="orientation">
      	    <intent-filter>
      	        <action android:name="com.kalo.myfirstapp.DEVICESCANNER"/>
      	        <category android:name="android.intent.category.DEFAULT"/>
      	    </intent-filter>
      	</activity>
      	
      	<activity android:name="bg.roboleague.mobile.RobotScreen"
      	    	  android:label="@string/app_name"
                android:configChanges="orientation">
      	    <intent-filter>
      	        <action android:name="com.kalo.myfirstapp.ROBOTSCREEN"/>
      	        <category android:name="android.intent.category.DEFAULT"/>
      	    </intent-filter>
      	</activity>
    </application>
</manifest>
